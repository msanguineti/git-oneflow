{
  "name": "git-oneflow",
  "version": "1.0.11",
  "description": "CLI tooling implementing GIT OneFlow branching model",
  "bin": {
    "gof": "bin/cli",
    "git-oneflow": "bin/cli"
  },
  "files": [
    "bin/cli"
  ],
  "scripts": {
    "pretest": "npm run lint",
    "test": "tsc && jest",
    "posttest": "npm run format",
    "clean": "rimraf bin",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "lint": "eslint . --cache --fix",
    "format": "prettier --loglevel warn --write .",
    "watch": "rollup -c --watch",
    "typecheck": "tsc",
    "prerelease": "GOF_CHICKENOUT=true npm test",
    "release": "standard-version",
    "postrelease": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msanguineti/git-oneflow.git"
  },
  "keywords": [
    "git",
    "oneflow",
    "branching",
    "model",
    "Adam",
    "Ruka"
  ],
  "author": "Mirco Sanguineti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/msanguineti/git-oneflow/issues"
  },
  "homepage": "https://github.com/msanguineti/git-oneflow#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.15",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "lint-staged": "^10.5.0",
    "prettier": "^2.1.2",
    "rollup": "^2.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^6.1.0",
    "cosmiconfig": "^7.0.0",
    "inquirer": "^7.3.3",
    "shelljs": "^0.8.4"
  }
}
