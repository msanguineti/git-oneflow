{
  "name": "git-oneflow",
  "version": "1.1.2",
  "description": "CLI tooling implementing GIT OneFlow branching model",
  "bin": {
    "gof": "bin/cli",
    "git-oneflow": "bin/cli"
  },
  "files": [
    "bin/cli"
  ],
  "scripts": {
    "pretest": "npm run lint",
    "test": "tsc && jest",
    "posttest": "npm run format",
    "clean": "rimraf bin",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "watch": "rollup -c --watch",
    "typecheck": "tsc",
    "prerelease": "GOF_DRY_RUN=true npm t",
    "release": "standard-version",
    "prepublishOnly": "npm run build && npm t",
    "lint": "eslint . --cache --fix --ext .ts",
    "format": "prettier --loglevel warn --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msanguineti/git-oneflow.git"
  },
  "keywords": [
    "git",
    "oneflow",
    "branching",
    "model",
    "Adam",
    "Ruka"
  ],
  "author": "Mirco Sanguineti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/msanguineti/git-oneflow/issues"
  },
  "homepage": "https://github.com/msanguineti/git-oneflow#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^27.5.0",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^12.4.2",
    "prettier": "^2.6.2",
    "rollup": "^2.75.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "commander": "^9.2.0",
    "cosmiconfig": "^7.0.1",
    "fude": "^2.0.0",
    "inquirer": "^8.2.4",
    "shelljs": "^0.8.5"
  }
}
