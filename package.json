{
  "name": "git-oneflow",
  "version": "0.10.8",
  "description": "CLI tooling implementing GIT OneFlow branching model",
  "bin": {
    "gof": "bin/cli",
    "git-oneflow": "bin/cli"
  },
  "files": [
    "bin/cli"
  ],
  "scripts": {
    "test": "jest",
    "clean": "rimraf bin",
    "prebuild": "run-s clean test",
    "build": "rollup -c",
    "lint": "eslint --fix 'src/**/*.ts' '__tests__/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts' '__tests__/**/*.ts'",
    "watch": "rollup -c --watch",
    "typecheck": "tsc",
    "prerelease": "run-s typecheck lint build",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msanguineti/git-oneflow.git"
  },
  "keywords": [
    "git",
    "oneflow",
    "branching",
    "model",
    "Adam",
    "Ruka"
  ],
  "author": "Mirco Sanguineti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/msanguineti/git-oneflow/issues"
  },
  "homepage": "https://github.com/msanguineti/git-oneflow#readme",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/find-up": "^4.0.0",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.1.4",
    "@types/shelljs": "^0.8.6",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rollup": "^2.0.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "standard-version": "^7.1.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "find-up": "^4.1.0",
    "inquirer": "^7.1.0",
    "shelljs": "^0.8.3",
    "yargs": "^15.3.0"
  }
}
