{
  "name": "git-oneflow",
  "version": "1.0.0",
  "description": "CLI tooling implementing GIT OneFlow branching model",
  "bin": {
    "gof": "bin/cli",
    "git-oneflow": "bin/cli"
  },
  "files": [
    "bin/cli"
  ],
  "scripts": {
    "pretest": "npm run lint && npm run format",
    "test": "jest",
    "clean": "rimraf bin",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "lint": "eslint --fix  \"*.js\" \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format": "prettier --write .",
    "watch": "rollup -c --watch",
    "typecheck": "tsc",
    "prerelease": "run-s typecheck lint format build",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msanguineti/git-oneflow.git"
  },
  "keywords": [
    "git",
    "oneflow",
    "branching",
    "model",
    "Adam",
    "Ruka"
  ],
  "author": "Mirco Sanguineti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/msanguineti/git-oneflow/issues"
  },
  "homepage": "https://github.com/msanguineti/git-oneflow#readme",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.2.1",
    "@types/shelljs": "^0.8.7",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rollup": "^2.8.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "standard-version": "^7.1.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "commander": "^5.1.0",
    "cosmiconfig": "^6.0.0",
    "inquirer": "^7.1.0",
    "shelljs": "^0.8.3"
  }
}
